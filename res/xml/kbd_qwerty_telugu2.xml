            android:codes="0x0C08" />
        <Key
            android:keyIcon="@drawable/telugu_0c0a"
            android:codes="0x0C0A" />
        <Key
            android:keyIcon="@drawable/telugu_0c2d"
            android:codes="0x0C2D" />
        <Key
            android:keyIcon="@drawable/telugu_0c19"
            android:codes="0x0C19"
            android:keyEdgeFlags="right" />
    </Row>
      
	<Row>
        <Key
            android:keyIcon="@drawable/telugu_0c18"
            android:codes="0x0C18"
            android:keyEdgeFlags="left" />
        <Key
            android:keyIcon="@drawable/telugu_0c27"
