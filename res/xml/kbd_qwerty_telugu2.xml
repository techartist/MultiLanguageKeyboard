    <Row android:keyWidth="14%p">
       <Key
            android:codes="@integer/key_fn"
            android:keyIcon="@drawable/telugu_more"
            android:keyWidth="15%p"
            android:isModifier="true"
            android:keyEdgeFlags="left" />
        <Key
            android:keyIcon="@drawable/telugu_0c56"
            android:codes="0x0C56" />
        <Key
            android:keyIcon="@drawable/telugu_0c55"
