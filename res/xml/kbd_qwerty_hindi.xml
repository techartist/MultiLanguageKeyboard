            android:popupCharacters=""/>
        <Key
            android:keyLabel="५"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""/>
        <Key
            android:keyLabel="६"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""
    		android:keyWidth="9%p"/>
        <Key
            android:keyLabel="७"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""
    		android:keyWidth="9%p"/>
        <Key
            android:keyLabel="८"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""/>
        <Key
            android:keyLabel="९"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""/>
        <Key
            android:keyLabel="०"
            android:popupKeyboard="@xml/kbd_popup_template"
