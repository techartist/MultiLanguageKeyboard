            android:popupCharacters=""/>
        <Key
            android:keyLabel="ॉ "
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""
            android:keyEdgeFlags="right"
    		android:keyWidth="9%p" />
    </Row>
       
    <Row
    	android:keyWidth="8%p">
        <Key
            android:keyLabel="ौ"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""
            android:keyEdgeFlags="left"
    		android:keyWidth="9%p" />
        <Key
            android:keyLabel="ै"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""/>
        <Key
            android:keyLabel="ा"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""/>
        <Key
            android:keyLabel="ी"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""/>
        <Key
            android:keyLabel="ू"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""/>
        <Key
            android:keyLabel="ब"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""
    		android:keyWidth="9%p"/>
        <Key
            android:keyLabel="ह"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""
    		android:keyWidth="9%p"/>
        <Key
            android:keyLabel="ग"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:popupCharacters=""/>
        <Key
